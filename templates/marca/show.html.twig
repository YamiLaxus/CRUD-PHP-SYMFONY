        </tbody>
    </table>

    <a href="{{ path('app_marca_index') }}" class="btn-nuevo">Volver</a>

    <a href="{{ path('app_marca_edit', {'idMarca': marca.idMarca}) }}" class="btn-nuevo">Editar</a>

    {{ include('marca/_delete_form.html.twig') }}
{% endblock %}
