{% extends 'base.html.twig' %}

{% block title %}Marca{% endblock %}

{% block body %}
<div id="header_container"><h1 class="title">Marca</h1></div>    

    <table class="table">
        <tbody>
            <tr>
                <th>ID</th>
                <td>{{ marca.idMarca }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ marca.descripcion }}</td>
