{% extends 'base.html.twig' %}

{% block title %}Zona{% endblock %}

{% block body %}
<div id="header_container"><h1 class="title">Zonas</h1></div>  

    <table class="table">
        <tbody>
            <tr>
                <th>IdZona</th>
                <td>{{ zona.idZona }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
